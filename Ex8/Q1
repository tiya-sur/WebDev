<script>
v1=parseInt(prompt("Number 1:"));
v2=parseInt(prompt("Number 2:"));
a=v1+v2
alert("addition: "+ a);
b=v1-v2
alert("subtraction: "+ b);
c=v1*v2
alert("multiplication: "+ c);
d=v1/v2
alert("Division: " + d);


</script>
<script>
age=(parseInt(prompt("Enter your age:")));
if(age>=18)
{
alert("eligible to drive!");
}
else{
alert("Not eligible to drive!");
}
</script>

