<script>
age=(parseInt(prompt("Enter your age:")));
if(age>=18)
{
alert("eligible to drive!");
}
else{
alert("Not eligible to drive!");
}
</script>
<script>
rate=45;
hours=(parseInt(prompt("Enter Labour Hours:")));
mat=(parseInt(prompt("Enter Cost of Materials:")));
b=rate*hours
a=b+mat;
alert("Total Charge: "+ a);
</script>

